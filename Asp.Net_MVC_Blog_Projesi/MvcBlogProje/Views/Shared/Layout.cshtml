
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Mini Blog</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,700|Playfair+Display:400,700,900" rel="stylesheet">
    <link rel="stylesheet" href="fonts/icomoon/style.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/magnific-popup.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/jquery-ui.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/fonts/flaticon/font/flaticon.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/aos.css">
    <link rel="stylesheet" href="~/miniBlogTemplate/css/style.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
</head>
<body>

    <div class="site-wrap">
        <div class="site-mobile-menu">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>

        <header class="site-navbar" role="banner">
            <div class="container-fluid">
                <div class="row align-items-center">

                    <div class="col-12 search-form-wrap js-search-form">
                        <form method="get" action="#">
                            <input type="text" id="s" class="form-control" placeholder="Search...">
                            <button class="search-btn" type="submit"><span class="icon-search"></span></button>
                        </form>
                    </div>
                    <div class="col-4 site-logo">
                        <a href="index.html" class="text-black h2 mb-0">Mini Blog</a>
                    </div>
                    <div class="col-8 text-right">
                        <nav class="site-navigation" role="navigation">
                            <ul class="site-menu js-clone-nav mr-auto d-none d-lg-block mb-0">
                                <li><a href="/Blog/Index">ANASAYFA</a></li>
                                <li><a href="/Blog/BlogByCategory/2/">YAZILIM</a></li>
                                <li><a href="/Blog/BlogByCategory/3/">KÜLTÜR & SANAT</a></li>
                                <li><a href="/Blog/BlogByCategory/4/">TARİH</a></li>
                                <li><a href="/Blog/BlogByCategory/5/">SEYAHAT</a></li>
                                <li><a href="/Blog/BlogByCategory/6/">FİLM DİZİ</a></li>
                                <li><a href="/Blog/BlogByCategory/7/">EKONOMİ</a></li>
                                <li><a href="/About/Index/">HAKKIMIZDA</a></li>
                                <li><a href="/Contact/Index/">İLEİTİŞİM</a></li>
                                <li class="d-none d-lg-inline-block"><a href="#" class="js-search-toggle"><span class="icon-search"></span></a></li>
                            </ul>
                        </nav>
                        <a href="#" class="site-menu-toggle js-menu-toggle text-black d-inline-block d-lg-none"><span class="icon-menu h3"></span></a>
                    </div>
                </div>
            </div>
        </header>
       
            @RenderBody()
        
      



    </div>
    <script src="~/miniBlogTemplate/js/jquery-3.3.1.min.js"></script>
    <script src="~/miniBlogTemplate/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/miniBlogTemplate/js/jquery-ui.js"></script>
    <script src="~/miniBlogTemplate/js/popper.min.js"></script>
    <script src="~/miniBlogTemplate/js/bootstrap.min.js"></script>
    <script src="~/miniBlogTemplate/js/owl.carousel.min.js"></script>
    <script src="~/miniBlogTemplate/js/jquery.stellar.min.js"></script>
    <script src="~/miniBlogTemplate/js/jquery.countdown.min.js"></script>
    <script src="~/miniBlogTemplate/js/jquery.magnific-popup.min.js"></script>
    <script src="~/miniBlogTemplate/js/bootstrap-datepicker.min.js"></script>
    <script src="~/miniBlogTemplate/js/aos.js"></script>
    <script src="~/miniBlogTemplate/js/main.js"></script>

</body>
</html>